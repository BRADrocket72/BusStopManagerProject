@model IEnumerable<RouteViewModel>

@{
    ViewBag.Title = "Routes List";
}

<div><b>Bus Routes</b><br /></div>
<table class="table table-hover">
    <tr>
        <th>Order </th>
    </tr>
    @foreach (RouteViewModel d in Model)
    {
            <tr>
                <td>@d.Order</td>
                <td><button>Delete</button><button>View on Map</button></td>
                <td></td>
            </tr>
    }
</table>